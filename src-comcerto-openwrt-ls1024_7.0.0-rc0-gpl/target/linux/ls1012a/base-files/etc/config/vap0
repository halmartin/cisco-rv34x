config wifi-vap vap0
	option interface       ''
	option disabled        '1'
	option radio_type      'radio0'
	option ssid            'Openwrt-mspd-vap0'
	option hidden_ssid     '0'
	option authentication  'Open'
	option encryption      'none'
	option ipaddr          '192.168.3.1'
	option netmask         '255.255.255.0'

