
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'vlan1'
	option ifname 'eth3.1'
	option proto 'static'
	option ipaddr '192.168.10.1'
	option netmask '255.255.255.0'
	option ip6addr 'fec0::1/64'

config interface 'wan1'
	option ifname 'eth2'
	option proto 'dhcp'
	option peerdns '1'
	option enable '1'
	option metric '1'

config interface 'wan16'
	option ifname 'eth2'
	option proto 'dhcpv6'
	option ipv6 '1'
	option peerdns '1'
	option enable '1'

config interface 'wan2'
	option ifname 'eth0'
	option proto 'dhcp'
	option peerdns '1'
	option enable '1'
	option metric '2'

config interface 'wan26'
	option ifname 'eth0'
	option proto 'dhcpv6'
	option ipv6 '1'
	option peerdns '1'
	option enable '1'

config interface 'usb1'
	option proto 'mobile'
	option demand '5'
	option enable '1'
	option metric '3'
	option device '/dev/modem1'
	option pppd_options 'noauth'

config interface 'usb2'
	option proto 'mobile'
	option enable '1'
	option metric '4'
	option device '/dev/modem2'
	option apn 'vzwinternet'
