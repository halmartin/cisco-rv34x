
config profile 'prof_Default'
	option profile_name 'prof_Default'
	option keying_mode 'auto'
	option ike_lifetime '28800s'
	option proto 'esp'
	option pfs '1'
	option ipsec_lifetime '3600s'
	option p1_prop 'aes128-sha1-modp1536'
	option p2_prop 'aes128-sha1-modp1536'

config clientGlobal 'clientGlobal_0'
	option client_status '0'
	option auto_retry '0'
	option retry_interval '120'
	option retry_limit '2'

config profile 'prof_Amazon_Web_Services'
	option profile_name 'prof_Amazon_Web_Services'
	option keying_mode 'auto'
	option ike_lifetime '28800s'
	option p1_prop 'aes128-sha1-modp1024'
	option ipsec_lifetime '3600s'
	option pfs '1'
	option proto 'esp'
	option p2_prop 'aes128-sha1-modp1024'

config profile 'prof_Microsoft_Azure'
	option profile_name 'prof_Microsoft_Azure'
	option keying_mode 'auto'
	option ike_lifetime '28800s'
	option p1_prop 'aes256-sha1-modp1024'
	option ipsec_lifetime '3600s'
	option pfs '0'
	option proto 'esp'
	option p2_prop 'aes256-sha1'

