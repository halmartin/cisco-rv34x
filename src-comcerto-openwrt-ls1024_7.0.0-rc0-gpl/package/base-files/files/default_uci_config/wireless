
config wifi-device 'wl0'
	option type 'broadcom'
	option disabled '0'
	option txpower '100'
	option hwmode 'b g n'
	option channel 'auto'
	option htmode '20'
	option frameburst '0'
	option wme_no_ack '0'
	option basic_rate '1.0 2.0 5.5 11.0'
	option transmit_rate '1.0 2.0 5.5 6.0 9.0 11.0 12.0 18.0 24.0 36.0 48.0 54.0'
	option ht_mcs 'ffffff'
	option cts_protection_mode '1'
	option beacon_int '100'
	option mu_mimo '0'
	option beamforming '1'
	option dtim_period '1'
	option frag '2346'
	option rts '2347'
	option wme_apsd '1'
	option maxassoc '50'

config wifi-device 'wl1'
	option type 'broadcom'
	option disabled '0'
	option txpower '100'
	option hwmode 'a n ac'
	option channel 'auto'
	option htmode '80'
	option frameburst '0'
	option wme_no_ack '0'
	option basic_rate '6.0 12.0 24.0'
	option transmit_rate '6.0 9.0 12.0 18.0 24.0 36.0 48.0 54.0'
	option ht_mcs 'ffffffff'
	option vht_mcs_1 'fff'
	option vht_mcs_2 'fff'
	option vht_mcs_3 'fff'
	option vht_mcs_4 'fff'
	option cts_protection_mode '1'
	option beacon_int '100'
	option mu_mimo '0'
	option beamforming '1'
	option dtim_period '1'
	option frag '2346'
	option rts '2347'
	option wme_apsd '1'
	option maxassoc '124'

config wifi-iface
	option device 'wl0'
	option mode 'ap'
	option disabled '0'
	option ssid 'csb1'
	option network 'lan'
	option hidden '0'
	option wmm '1'
	option vlanid '1'
	option isolate '1'
	option pmf '0'
	option wps '1'
	option captive '0'
	option captive_portal_profile 'Default_Portal_Profile'
	option encryption 'none'
	option mac_filter '1'
	option macpolicy '0'

