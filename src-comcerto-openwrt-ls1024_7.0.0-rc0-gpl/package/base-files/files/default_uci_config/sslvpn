
config sslvpn 'general'
	option enable '0'
	option gwport '8443'
	option certfile 'Default'
	option domain 'yourdomain.com'
	option banner 'Hello, welcome!'
	option usecryptodev '1'
	option maxsessions '10'
	option idletimeout '2100'
	option sessiontimeout '0'
	option cdpdtimeout '300'
	option gwdpdtimeout '300'
	option keepalive '30'
	option leaseduration '43200'
	option maxmtu '1406'
	option rekeymethod 'ssl'
	option rekeyinterval '3600'
	option cmdname '/tmp/sslvpnFW'
	option addopt 'add'
	option delopt 'del'

config policy 'policy_0'
	option policyname 'SSLVPNDefaultPolicy'
	option dnsprimary '192.168.1.1'
	option ieproxypolicy 'disabled'
	option enable '0'

